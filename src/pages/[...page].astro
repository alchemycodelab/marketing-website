---
import { getStaticPaths, getPage } from '../services/builder.js';
import Page from '../layouts/page.astro';
import BuilderServer from '../components/Builder/BuilderServer.jsx';

export { getStaticPaths };

const { pages } = Astro.props;
const { url } = Astro.request;
const page = await getPage(url.pathname);
// console.log('pages:prod', page.name, import.meta.env);
---

<Page pages={pages} page={page}>
	<BuilderServer page={page} />
</Page>
